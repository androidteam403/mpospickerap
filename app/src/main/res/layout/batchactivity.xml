<?xml version="1.0" encoding="utf-8"?>
<layout>

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen60dp"
                android:background="#01bfbf"
                android:orientation="horizontal">

                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginLeft="@dimen/dimen15dp"
                    android:layout_marginTop="@dimen/dimen22dp"
                    android:src="@drawable/left_forward_white_arrow" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimen10dp"
                    android:layout_marginTop="@dimen/dimen22dp"
                    android:text="BATCH ID'S"
                    android:textColor="@color/white"
                    android:textSize="@dimen/dimen16sp"
                    android:textStyle="bold" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen50dp"
                android:orientation="horizontal">

                <TextView
                    android:id="@+id/tablet_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimen20dp"
                    android:layout_marginTop="@dimen/dimen14sp"
                    android:text="Augmentin 625 Duo Tablet"
                    android:textColor="@color/black"
                    android:textSize="16dp"
                    android:textStyle="bold" />

                <ImageView
                    android:layout_width="@dimen/dimen20dp"
                    android:layout_height="@dimen/dimen20dp"
                    android:layout_marginLeft="110dp"
                    android:layout_marginTop="15dp"
                    android:src="@drawable/black_delete_icon" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen3dp"
                android:background="#A9A9A9" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="27dp"
                android:layout_marginTop="@dimen/dimen10dp"
                android:text="Not found Batch ID"
                android:textColor="@color/black"
                android:textSize="@dimen/dimen14sp" />

            <LinearLayout
                android:layout_width="320dp"
                android:layout_height="@dimen/dimen40dp"
                android:layout_marginLeft="@dimen/dimen20dp"
                android:layout_marginTop="@dimen/dimen5dp"
                android:background="#e6e6e6"
                android:orientation="horizontal"
                android:paddingLeft="@dimen/dimen10dp">

                <EditText
                    android:layout_width="280dp"
                    android:layout_height="match_parent"
                    android:background="#e6e6e6"
                    android:hint="Enter Batch ID"
                    android:paddingStart="@dimen/dimen10dp" />

                <ImageView
                    android:id="@+id/search_batchlistitem"
                    android:layout_width="@dimen/dimen22dp"
                    android:layout_height="@dimen/dimen22dp"
                    android:layout_marginTop="@dimen/dimen10dp"
                    android:src="@drawable/next_order" />
            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen3dp"
                android:layout_marginTop="@dimen/dimen10dp"
                android:background="#A9A9A9" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/dimen20dp"
                android:layout_marginTop="@dimen/dimen20dp"
                android:text="Select from batch list"
                android:textColor="@color/black"
                android:textSize="@dimen/dimen14sp" />

            <RadioGroup
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <RadioButton
                    android:id="@+id/firstitem_batchlist"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dimen20dp"
                    android:layout_marginTop="@dimen/dimen15dp"
                    android:text="04062010Z HEALTH"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dimen16sp"
                    android:visibility="gone" />

                <RelativeLayout
                    android:id="@+id/firstitem_batchlistpriority"
                    android:layout_width="320dp"
                    android:layout_height="@dimen/dimen50dp"
                    android:layout_marginLeft="19dp"
                    android:layout_marginTop="13dp"
                    android:background="@drawable/litegreenstroke">

                    <ImageView
                        android:layout_width="23dp"
                        android:layout_height="23dp"
                        android:layout_marginLeft="@dimen/dimen7dp"
                        android:layout_marginTop="@dimen/dimen14sp"
                        android:src="@drawable/ic_sharp_circle_24" />

                    <ImageView
                        android:layout_width="@dimen/dimen12sp"
                        android:layout_height="@dimen/dimen12sp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginTop="19dp"
                        android:src="@drawable/tick_mark" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/dimen50dp"
                        android:layout_marginLeft="35dp"
                        android:layout_marginTop="14dp"
                        android:text="04062010Z HEALTH"

                        android:textColor="@color/black"
                        android:textSize="@dimen/dimen16sp" />

                </RelativeLayout>

                <RadioButton
                    android:id="@+id/seconditem_batchlist"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dimen50dp"
                    android:layout_marginLeft="@dimen/dimen20dp"
                    android:layout_marginTop="@dimen/dimen10dp"
                    android:text="04062010Z HEALTH"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dimen16sp"
                    android:visibility="gone" />

                <RelativeLayout
                    android:id="@+id/seconditem_batchlistpriority"
                    android:layout_width="320dp"
                    android:layout_height="@dimen/dimen50dp"
                    android:layout_marginLeft="19dp"
                    android:layout_marginTop="13dp"
                    android:background="@drawable/litegreenstroke">

                    <ImageView
                        android:layout_width="23dp"
                        android:layout_height="23dp"
                        android:layout_marginLeft="@dimen/dimen7dp"
                        android:layout_marginTop="@dimen/dimen14sp"
                        android:src="@drawable/ic_sharp_circle_24" />

                    <ImageView
                        android:layout_width="@dimen/dimen12sp"
                        android:layout_height="@dimen/dimen12sp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginTop="19dp"
                        android:src="@drawable/tick_mark" />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/dimen50dp"
                        android:layout_marginLeft="35dp"
                        android:layout_marginTop="14dp"
                        android:text="04062010Z HEALTH"

                        android:textColor="@color/black"
                        android:textSize="@dimen/dimen16sp" />
                </RelativeLayout>

                <RadioButton
                    android:id="@+id/thirditem_batchlist"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dimen50dp"
                    android:layout_marginLeft="@dimen/dimen20dp"
                    android:layout_marginTop="@dimen/dimen10dp"
                    android:text="04062010Z HEALTH"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dimen16sp" />

                <RelativeLayout
                    android:id="@+id/thirditem_batchlistpriority"
                    android:layout_width="320dp"
                    android:layout_height="@dimen/dimen50dp"
                    android:layout_marginLeft="19dp"
                    android:layout_marginTop="13dp"
                    android:background="@drawable/litegreenstroke"
                    android:visibility="gone">

                    <ImageView
                        android:layout_width="23dp"
                        android:layout_height="23dp"
                        android:layout_marginLeft="@dimen/dimen7dp"
                        android:layout_marginTop="@dimen/dimen14sp"
                        android:src="@drawable/ic_sharp_circle_24" />

                    <ImageView
                        android:layout_width="@dimen/dimen12sp"
                        android:layout_height="@dimen/dimen12sp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginTop="19dp"
                        android:src="@drawable/tick_mark" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/dimen50dp"
                        android:layout_marginLeft="35dp"
                        android:layout_marginTop="14dp"
                        android:text="04062010Z HEALTH"

                        android:textColor="@color/black"
                        android:textSize="@dimen/dimen16sp" />
                </RelativeLayout>


                <RadioButton
                    android:id="@+id/fourthitem_batchlist"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dimen50dp"
                    android:layout_marginLeft="@dimen/dimen20dp"
                    android:layout_marginTop="@dimen/dimen10dp"
                    android:text="04062010Z HEALTH"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dimen16sp" />

                <RelativeLayout
                    android:id="@+id/fourthitem_batchlistpriority"
                    android:layout_width="320dp"
                    android:layout_height="@dimen/dimen50dp"
                    android:layout_marginLeft="19dp"
                    android:layout_marginTop="13dp"
                    android:background="@drawable/litegreenstroke"
                    android:visibility="gone">
                    <ImageView
                        android:layout_width="23dp"
                        android:layout_height="23dp"
                        android:layout_marginLeft="@dimen/dimen7dp"
                        android:layout_marginTop="@dimen/dimen14sp"
                        android:src="@drawable/ic_sharp_circle_24" />

                    <ImageView
                        android:layout_width="@dimen/dimen12sp"
                        android:layout_height="@dimen/dimen12sp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginTop="19dp"
                        android:src="@drawable/tick_mark" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/dimen50dp"
                        android:layout_marginLeft="35dp"
                        android:layout_marginTop="14dp"
                        android:text="04062010Z HEALTH"
                        android:textColor="@color/black"
                        android:textSize="@dimen/dimen16sp" />
      </RelativeLayout>


                <RadioButton
                    android:id="@+id/fifthitem_batchlist"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dimen50dp"
                    android:layout_marginLeft="19dp"
                    android:layout_marginTop="@dimen/dimen10dp"
                    android:text="04062010Z HEALTH"
                    android:textColor="@color/black"
                    android:textSize="@dimen/dimen16sp" />

                <RelativeLayout
                    android:id="@+id/fifthitem_batchlistpriority"
                    android:layout_width="320dp"
                    android:layout_height="@dimen/dimen50dp"
                    android:layout_marginLeft="19dp"
                    android:layout_marginTop="13dp"
                    android:background="@drawable/litegreenstroke"
                    android:visibility="gone">

                    <ImageView
                        android:layout_width="23dp"
                        android:layout_height="23dp"
                        android:layout_marginLeft="@dimen/dimen7dp"
                        android:layout_marginTop="@dimen/dimen14sp"
                        android:src="@drawable/ic_sharp_circle_24" />

                    <ImageView
                        android:layout_width="@dimen/dimen12sp"
                        android:layout_height="@dimen/dimen12sp"
                        android:layout_marginLeft="12dp"
                        android:layout_marginTop="19dp"
                        android:src="@drawable/tick_mark" />
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="@dimen/dimen50dp"
                        android:layout_marginLeft="35dp"
                        android:layout_marginTop="14dp"
                        android:text="04062010Z HEALTH"

                        android:textColor="@color/black"
                        android:textSize="@dimen/dimen16sp" />
                </RelativeLayout>


            </RadioGroup>
        </LinearLayout>
    </RelativeLayout>
</layout>